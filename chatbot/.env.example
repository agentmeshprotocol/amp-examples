# Environment Variables for Multi-Agent Chatbot System
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_TEMPERATURE=0.1

# AMP Network Configuration
AMP_REGISTRY_URL=http://localhost:8000
AMP_TRANSPORT_TYPE=http
AMP_HEARTBEAT_INTERVAL=30
AMP_AUTO_RECONNECT=true

# System Configuration
LOG_LEVEL=INFO
MAX_CONCURRENT_CONVERSATIONS=100
ENABLE_METRICS=true

# Security Configuration
ENABLE_AUTHENTICATION=false
SESSION_TIMEOUT=3600
RATE_LIMIT_PER_HOUR=100

# Database Configuration (optional)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Monitoring Configuration (optional)
PROMETHEUS_URL=http://localhost:9090
GRAFANA_URL=http://localhost:3000

# Production Configuration
ENVIRONMENT=development
DEBUG=true
SENTRY_DSN=

# Agent-specific Configuration
ROUTER_AGENT_MODEL=gpt-3.5-turbo
FAQ_AGENT_MODEL=gpt-3.5-turbo
SALES_AGENT_MODEL=gpt-3.5-turbo
TECH_SUPPORT_AGENT_MODEL=gpt-4

# External Integrations (optional)
SLACK_BOT_TOKEN=
DISCORD_BOT_TOKEN=
TELEGRAM_BOT_TOKEN=

# CRM Integration (optional)
SALESFORCE_CLIENT_ID=
SALESFORCE_CLIENT_SECRET=
HUBSPOT_API_KEY=

# Ticket System Integration (optional)
JIRA_URL=
JIRA_USERNAME=
JIRA_API_TOKEN=

# Analytics Integration (optional)
GOOGLE_ANALYTICS_ID=
MIXPANEL_TOKEN=